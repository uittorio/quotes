  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "axios": "0.18.0",
    "do-nothing-loader": "1.0.0",
    "electron": "5.0.1",
    "electron-builder": "20.39.0",
    "electron-store": "3.2.0",
    "electron-webpack": "2.6.2",
    "jasmine": "3.4.0",
    "jasmine-ts": "0.3.0",
    "node-sass": "4.12.0",
    "sass-loader": "7.1.0",
